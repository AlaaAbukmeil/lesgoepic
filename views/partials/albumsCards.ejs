<div class="title">
  <h1>Albums</h1>
</div>
<div class="events row">

  <% albums.forEach(function(album){%>

  <div class="col-lg-4 col-md-6 col-6">
    <a href="<%=album.drive_link%>" method="get">
      <div class="card">
        <img src="https://drive.google.com/uc?export=view&id=<%=album.image%>" class="card-img-top cardImage" alt="...">
        <div class="card-body">
          <h6 class="date formNotes"><%=album.date%></h6>
          <h4 class="name descriptionAlbums" ><%=album.name%></h4>
        </div>
      </div>
    </a>
  </div>
  <%   })%>
